; Aero  ALL cursor packs
[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Files.Blue, Files.Default, 	Files.Green, 	Files.Pink, Files.Purple, 	Files.Random, 	Files.Red,	Files.Silver,	Files.Yellow
AddReg    = Reg.Blue, 	Reg.Default, 	Reg.Green, 		Reg.Pink, 	Reg.Purple, 	Reg.Random, 	Reg.Red,	Reg.Silver,			Reg.Yellow

[DestinationDirs]
Files.Blue    = 10, "Cursors\Aero\Blue"
Files.Default = 10, "Cursors\Aero\Default"
Files.Green	  = 10, "Cursors\Aero\Green"
Files.Pink	  = 10, "Cursors\Aero\Pink"
Files.Purple  = 10, "Cursors\Aero\Purple"
Files.Random  = 10, "Cursors\Aero\Random"
Files.Red     = 10, "Cursors\Aero\Red"
Files.Silver  = 10, "Cursors\Aero\Silver"
Files.Yellow  = 10, "Cursors\Aero\Yellow"

[Reg.Blue]
HKCU,"%cur%","Aero Blue",,"%BLUE_DIR%\%pointer%,%BLUE_DIR%\%help%,%BLUE_DIR%\%work%,%BLUE_DIR%\%busy%,%BLUE_DIR%\%cross%,%BLUE_DIR%\%Text%,%BLUE_DIR%\%Hand%,%BLUE_DIR%\%unavailiable%,%BLUE_DIR%\%Vert%,%BLUE_DIR%\%Horz%,%BLUE_DIR%\%Dgn1%,%BLUE_DIR%\%Dgn2%,%BLUE_DIR%\%move%,%BLUE_DIR%\%alternate%,%BLUE_DIR%\%link%"

